{
  "version": 2,
  "builds": [
    {
      "src": "backend/backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/backend/wsgi.py"
    }
  ],
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "cd backend && python manage.py collectstatic --noinput"
}